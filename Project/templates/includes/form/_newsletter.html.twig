{% if form is not null %}
	{{ form_start(form) }}
	<div class="form-group">
		{{ form_row(form.consent) }}
	</div>
	<div class="footer-group">
		<div class='form-group'>
			{{ form_widget(form.email) }}
		</div>
		<div class="form-group">
			{{ form_widget(form.submit) }}
		</div>
	</div>
	{{ form_end(form) }}
{% endif %}
